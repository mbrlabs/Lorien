boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.12549, 0.129412, 0.141176, 1 )
config/icon="res://Assets/icon.png"

[autoload]

BrushStrokeTexture="*res://BrushStroke/BrushStrokeTexture.gd"
